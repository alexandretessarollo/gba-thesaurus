	<prefLabel xmlns="http://www.w3.org/2004/02/skos/core#" xml:lang="de">Ummantelter Porphyroklast mit monokliner Symmetrie</prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geolba.ac.at/structure/406">
	<broader xmlns="http://www.w3.org/2004/02/skos/core#" rdf:resource="http://resource.geolba.ac.at/structure/31"/>
